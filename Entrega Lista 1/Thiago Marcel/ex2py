numero = int (input("Digite um numero inteiro positivo: "))
if numero >=0:
    if numero == 0 or numero == 1:
        resultado = 1
    else: 
        resultado = 1 
        for i in range (2, numero + 1):
            resultado *= i
    print(f"fatorial de {numero} é {resultado}")
else:
    print("O numero não é valido")
     
